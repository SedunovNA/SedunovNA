import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    double x;
	    Scanner Scanner = new Scanner(System.in);
	    System.out.println("Введите число в x");
	    x = Scanner.nextDouble();
if (Math.abs(x-5) < 4.5)
System.out.println("true");
else 
System.out.println("false");
if ((x-5)>=4.5)
System.out.println("true");
else 
System.out.println("false");

              }
	}
