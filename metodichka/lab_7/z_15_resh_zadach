import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    double kg, sm, bmi;
	    final int sm_v_metr=100; // в 100 см 1 м
	    Scanner Scanner = new Scanner(System.in);
	    System.out.println("Введите вес в кг");
	    kg = Scanner.nextDouble();
	    System.out.println("Введите рот в см");
	    sm = Scanner.nextDouble();

bmi = kg / (sm / sm_v_metr * sm / sm_v_metr);

System.out.println("BMI равно " + bmi);
if (bmi < 18.5)
 System.out.println("Недостаточный вес");
 else if (bmi < 25)
 System.out.println("норма");
 else if (bmi < 30)
 System.out.println("избыточный вес");
 else
 System.out.println("ожирение");

              }
	}
