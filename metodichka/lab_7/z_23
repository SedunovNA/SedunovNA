import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    int age,ticketprice=0;
	    Scanner Scanner = new Scanner(System.in);
	    System.out.println("Введите число в x");
	    age = Scanner.nextInt();
System.out.println(ticketprice = (age > 16) ? 20 : 10);
              }
	}
