import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    boolean b = true;
	    System.out.println("допустимы ли следющие преобразования");
	   i = (int) b;
	   System.out.println(i);
	   int i = 1;
	   System.out.println(i);
	   boolean b = (boolean)i;
	   System.out.println(i);

	}
}
