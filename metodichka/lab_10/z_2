public class Main {
    public static void main (String[]args) {
        int n1, m1;
        method1(1,2);
    }
    public static void method1(int n, int m) {
        n +=m;
        m = method2(3);
        System.out.println (m);
    }
    public static int method2(int n) {
        if (n > 0) {return 1;}
        else if (n == 0) {return 0;}
        else if (n < 0) {return -1;}
        else return -100;
    }
}
