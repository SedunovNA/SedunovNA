public class Main {
    public static void main (String[]args) {
        m(4, 5);
        m(4, 5.4);
        m(4.5, 5.4);
    }
    public static void m(double x, double y) {
        double c;
        c = (x > y) ? x : y;
        System.out.println(c + " 1");
       
    }
    public static void m(int x, double y) {
        double c;
        c = (x > y) ? x : y;
        System.out.println(c + " 2");
    }
}
